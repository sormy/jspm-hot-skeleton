{
  "jspm": {
    "name": "app",
    "main": "src/index.jsx",
    "dependencies": {
      "bootstrap": "github:twbs/bootstrap@4.0.0-alpha.4",
      "font-awesome": "npm:font-awesome@^4.6.3",
      "open-sans": "github:FontFaceKit/open-sans@^1.4.2",
      "react-dom": "npm:react-dom@^15.3.2"
    },
    "devDependencies": {
      "babel-preset-react": "npm:babel-preset-react@^6.11.1",
      "plugin-babel": "npm:systemjs-plugin-babel@^0.0.15",
      "plugin-css": "github:systemjs/plugin-css@^0.1.29",
      "plugin-sass": "local:plugin-sass@0.4.6",
      "react-hot-loader": "npm:react-hot-loader@^3.0.0-beta.5",
      "systemjs-hot-reloader-ex": "npm:systemjs-hot-reloader-ex@^2.0.0"
    },
    "peerDependencies": {
      "assert": "github:jspm/nodelibs-assert@^0.2.0-alpha",
      "buffer": "github:jspm/nodelibs-buffer@^0.2.0-alpha",
      "child_process": "github:jspm/nodelibs-child_process@^0.2.0-alpha",
      "constants": "github:jspm/nodelibs-constants@^0.2.0-alpha",
      "crypto": "github:jspm/nodelibs-crypto@^0.2.0-alpha",
      "domain": "github:jspm/nodelibs-domain@^0.2.0-alpha",
      "events": "github:jspm/nodelibs-events@^0.2.0-alpha",
      "fs": "github:jspm/nodelibs-fs@^0.2.0-alpha",
      "graceful-fs": "npm:graceful-fs@^4.1.6",
      "http": "github:jspm/nodelibs-http@^0.2.0-alpha",
      "https": "github:jspm/nodelibs-https@^0.2.0-alpha",
      "module": "github:jspm/nodelibs-module@^0.2.0-alpha",
      "os": "github:jspm/nodelibs-os@^0.2.0-alpha",
      "path": "github:jspm/nodelibs-path@^0.2.0-alpha",
      "process": "github:jspm/nodelibs-process@^0.2.0-alpha",
      "react": "npm:react@^15.3.2",
      "stream": "github:jspm/nodelibs-stream@^0.2.0-alpha",
      "string_decoder": "github:jspm/nodelibs-string_decoder@^0.2.0-alpha",
      "url": "github:jspm/nodelibs-url@^0.2.0-alpha",
      "util": "github:jspm/nodelibs-util@^0.2.0-alpha",
      "vm": "github:jspm/nodelibs-vm@^0.2.0-alpha",
      "zlib": "github:jspm/nodelibs-zlib@^0.2.0-alpha"
    },
    "overrides": {
      "github:FontFaceKit/open-sans@1.4.2": {
        "main": "open-sans.css!",
        "dependencies": {
          "css": "^0.1.5"
        },
        "registry": "jspm"
      },
      "github:HubSpot/tether@1.3.7": {
        "main": "js/tether",
        "directories": {
          "lib": "dist"
        },
        "meta": {
          "js/tether.js": {
            "exports": "Tether",
            "format": "global"
          }
        }
      },
      "github:twbs/bootstrap@4.0.0-alpha.4": {
        "files": null,
        "ignore": [
          "dist/js/npm"
        ],
        "directories": {
          "lib": ""
        },
        "dependencies": {
          "jquery": "2",
          "tether": "github:HubSpot/tether@^1.1.1"
        },
        "meta": {
          "js/src/*.js": {
            "deps": [
              "jquery",
              "tether"
            ],
            "exports": "$"
          }
        },
        "format": "detect",
        "map": {}
      },
      "npm:browserify-zlib@0.1.4": {
        "dependencies": {
          "readable-stream": "^2.0.2",
          "pako": "~0.2.0"
        },
        "map": {
          "_stream_transform": "readable-stream/transform"
        }
      },
      "npm:debug@2.2.0": {
        "main": "browser.js",
        "jspmNodeConversion": false,
        "format": "cjs",
        "map": {
          "./browser.js": {
            "node": "./node.js"
          },
          "./node.js": {
            "browser": "./browser.js"
          },
          "fs": "@node/fs",
          "net": "@node/net",
          "tty": "@node/tty",
          "util": "@node/util"
        }
      },
      "npm:font-awesome@4.6.3": {
        "main": "css/font-awesome.css!",
        "jspmNodeConversion": false,
        "registry": "jspm",
        "dependencies": {
          "css": "0.1"
        }
      },
      "npm:inherits@2.0.3": {
        "ignore": [
          "test.js"
        ]
      },
      "npm:jquery@2.2.4": {
        "format": "amd"
      },
      "npm:lodash@4.16.4": {
        "map": {
          "buffer": "@empty",
          "process": "@empty"
        }
      },
      "npm:ms@0.7.1": {
        "jspmNodeConversion": false,
        "format": "cjs"
      },
      "npm:react-hot-loader@3.0.0-beta.6": {
        "dependencies": {
          "babel-template": "^6.7.0",
          "react": "^15.0.2",
          "react-proxy": "^3.0.0-alpha.0",
          "global": "^4.3.0",
          "react-deep-force-update": "^2.0.1",
          "redbox-react": "^1.2.5"
        }
      },
      "npm:sass.js@0.9.13": {
        "map": {
          "ws": "@empty"
        },
        "meta": {
          "dist/sass.worker.js": {
            "format": "global"
          }
        }
      }
    }
  },
  "devDependencies": {
    "browser-sync": "^2.17.3",
    "bs-systemjs-hot-reloader": "^1.0.3",
    "jspm": "^0.17.0-beta.29"
  },
  "scripts": {
    "clean": "rm -rf dist && mkdir dist",
    "build": "npm run clean && npm run build:jspm && npm run build:index",
    "build:jspm": "jspm build app dist/app.js --minify --skip-source-maps",
    "build:index": "cp -f index.dist.html dist/index.html",
    "serve": "npm run build && node serve.dist.js",
    "start": "node serve.js"
  }
}
